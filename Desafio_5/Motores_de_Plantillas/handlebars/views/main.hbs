<table class="table table-dark">
  <thead>
    <tr class="text-center">
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Foto</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
      <tr class="text-center">
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td><img width="36" height="36" src={{this.thumbnail}}></td>
      </tr>
    {{/each}}
  </tbody>
</table>
<p class="text-center">{{noData.title}}</p>